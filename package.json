{
  "name": "blog",
  "version": "1.0.0",
  "description": "improve programming level in practice and thinking",
  "main": "index.js",
  "scripts": {
    "test": "nyc mocha -t 5000 --exit",
    "coverall": "nyc report --reporter=text-lcov | coveralls",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "addChangelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "node app.js"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "test",
        "chore"
      ],
      "scope": {
        "required": false,
        "allowed": [
          "*"
        ],
        "validate": false,
        "mutiple": false
      },
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": "",
      "autoFix": false
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "author": "daiqingyun",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.1.2",
    "conventional-changelog": "^2.0.1",
    "coveralls": "^3.0.0",
    "debug": "^3.2.6",
    "eslint": "^5.9.0",
    "eslint-plugin-html": "^4.0.6",
    "husky": "^1.0.0-rc.13",
    "lint-staged": "^7.2.0",
    "mocha": "^5.1.1",
    "nyc": "^11.7.1",
    "supertest": "^3.0.0",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "@log4js-node/smtp": "^1.0.1",
    "axios": "^0.18.0",
    "bloom-redis": "^0.0.1",
    "bloomfilter": "^0.0.18",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "charm": "^1.0.2",
    "color-name": "^1.1.3",
    "commander": "^2.19.0",
    "config": "file:./config",
    "connect": "^3.6.6",
    "corn": "^0.0.1",
    "cron": "^1.4.1",
    "debug": "^3.1.0",
    "escape-string-regexp": "^1.0.5",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "file-stream-rotator": "^0.2.1",
    "finalhandler": "^1.1.1",
    "iconv-lite": "^0.4.24",
    "inquirer": "^5.2.0",
    "ioredis": "^4.0.0",
    "joi": "^13.2.0",
    "jsonwebtoken": "^8.2.1",
    "koa": "^2.5.2",
    "koa-compose": "^4.1.0",
    "koa-static": "^5.0.0",
    "kue": "^0.11.6",
    "lighthouse": "^2.9.4",
    "lodash": "^4.17.11",
    "log4js": "^3.0.3",
    "minimist": "^1.2.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "mongoose": "^5.0.15",
    "morgan": "^1.9.0",
    "multer": "^1.3.1",
    "node-uuid": "^1.4.8",
    "parseurl": "^1.3.2",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.3",
    "puppeteer": "^1.4.0",
    "qiniu": "^7.2.1",
    "qs": "^6.6.0",
    "redis": "^2.8.0",
    "redis-rebloom": "^1.0.0",
    "request": "^2.88.0",
    "stacktrace-js": "^2.0.0",
    "utils-merge": "^1.0.1",
    "uuid": "^3.3.2"
  }
}
